<a href="/products">Products</a>
<hr>
<ul>
  {{#if carrito}}
    <ul>
      {{#each carrito.products}}
        <li>
          <h2>{{this.productId.title}}</h2>
          <p>Descripcion: {{this.productId.description}}</p>
          <p>Precio: {{this.productId.price}}</p>
          <p>Ruta de imagenes : {{this.productId.thumbnail}}</p>
          <p>Codigo : {{this.productId.code}}</p>
          <p>Categoría : {{this.productId.category}}</p>
          <p>Cantidad: {{this.qty}}</p>

        </li>
      {{/each}}
    </ul>
  {{else}}
    <p>El carrito está vacío.</p>
  {{/if}}